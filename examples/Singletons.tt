module

Bool : Type
Bool = {'true [] | 'false []}

IsTrue : Bool -> Type
IsTrue = split
       	 'true _ -> []
	 'false _ -> {}

true : Bool (= 'true ())
true = 'true ()


not : Bool -> Bool
not = split
       	 'true _ -> 'false ()
	 'false _ -> 'true ()

not2 : (Bool -> Bool) (= not)
not2 = not

not3 : (x : Bool) -> Bool (= not x)
not3 = not

