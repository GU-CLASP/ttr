\(params : [ Ind : Type
           ; spkr0 : Ind
           ; addr0 : Ind
           ; ]) ->

module

open params


turn_change : [spkr : Ind; addr : Ind] -> [spkr : Ind; addr : Ind]
turn_change r = (spkr = r.addr, addr = r.spkr)

initialize : [] -> [spkr : Ind; addr : Ind]
initialize _ = (spkr = spkr0, addr = addr0)

